{
  "name": "soma",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "apps/*",
    "packages/config/*",
    "packages/*"
  ],
  "engines": {
    "node": "^18.15.0"
  },
  "packageManager": "npm@9.5.0",
  "scripts": {
    "install:expo": "cd apps/expo && npm install --prefix .",
    "postinstall": "npm run install:expo && npm run db:generate",
    "dev": "turbo dev --parallel",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix && manypkg fix",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push"
  },
  "dependencies": {
    "googleapis": "^118.0.0",
    "turbo": "^1.11.0"
  }
}
