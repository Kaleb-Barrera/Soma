{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "db:generate": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "db:push": {
      "inputs": [
        "prisma/schema.prisma"
      ],
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "^db:generate"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "install:locally": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "lint:fix": {}
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY"
  ]
}
